{
  "name": "@harshalkatakiya/prettier-config",
  "version": "1.0.19",
  "type": "module",
  "description": "Harshal Katakiya's Prettier config",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "add": "dist/add.js"
  },
  "prettier": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --fix",
    "prettier:check": "prettier -c .",
    "prettier": "prettier -w -u .",
    "prepare": "husky",
    "release": "npm version patch && npm publish --access public --tag latest"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "inquirer": "^12.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "lint-staged": {
    "**/*": "prettier -w -u ."
  },
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/@harshalkatakiya/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harshalkatakiya/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/Harshalkatakiya/prettier-config/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": {
    "name": "Harshal Katakiya",
    "email": "katakiyaharshl001@gmail.com",
    "url": "https://github.com/Harshalkatakiya"
  },
